<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <Reference Include="Leadtools.Twain" Condition="'$(Platform)' == 'x86' Or ('$(Platform)' == 'AnyCPU' And ('$(Prefer32Bit)' == 'True' Or ('$(Use64BitIISExpress)' == 'False' And '$(Use64BitIISExpress)' != '')))">
      <HintPath>$(MSBuildThisFileDirectory)..\net40\x86\Leadtools.Twain.dll</HintPath>
    </Reference>
    <Reference Include="Leadtools.Twain" Condition="'$(Platform)' == 'x64' Or ('$(Platform)' == 'AnyCPU' And (('$(Prefer32Bit)' == 'False' And '$(Prefer32Bit)' != '') Or '$(Use64BitIISExpress)' == 'True'))">
      <HintPath>$(MSBuildThisFileDirectory)..\net40\x64\Leadtools.Twain.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>